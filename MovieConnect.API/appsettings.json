{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApiKey": "YourSecureApiKey12345",

  "MovieProvidersOptions": {
    "MovieDetailProviders": [
      {
        "Name": "omdb",
        "ApiKey": "OMDB_API_KEY",
        "ApiUrl": "http://www.omdbapi.com/",
        "IsActive": true,
        "HttpClientPolicies": {
          "RetryCount": 3,
          "RetryDelaySeconds": 3,
          "CircuitBreakerFailureThreshold": 5,
          "CircuitBreakerDurationSeconds": 5
        }
      }
    ],
    "MovieVideoProviders": [
      {
        "Name": "youtube",
        "ApiKey": "YOUTUBE_API_KEY",
        "ApiUrl": "https://www.googleapis.com/youtube/v3/",
        "IsActive": true,
        "HttpClientPolicies": {
          "RetryCount": 3,
          "RetryDelaySeconds": 3,
          "CircuitBreakerFailureThreshold": 5,
          "CircuitBreakerDurationSeconds": 5
        }
      }
    ]
  },
  "AllowedHosts": "*"
}